{
  "name": "watson-assistant",
  "version": "1.0.0",
  "main": "server/server.js",
  "homepage": "https://watson-assistant.mybluemix.net/",
  "author": "Gurvinder Singh",
  "contributors": [
    "Gurvinder Singh <gurvsin3@in.ibm.com> (http://www.gurvinder.info)"
  ],
  "engines": {
    "node": "12.22.0",
    "npm": "6.14.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "scripts": {
    "browsersync": "browser-sync start --proxy 'localhost:3000' --port 3001 --files 'dist/client/**/*.*'",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "build:client": "(cd ./client && npm run build)",
    "start": "node .",
    "serve": "(cd ./client && npm run build) && concurrently \"node .\" \"sleep 5 && opener http://localhost:3000\"",
    "watch:client": "(cd ./client && npm run develop)",
    "watch:server": "nodemon --watch ./server --watch ./common",
    "init": "npm install",
    "develop": "concurrently \"npm run watch:client\" \"npm run watch:server\" \"sleep 20 && npm run browsersync\"",
    "test": "jasmine"
  },
  "dependencies": {
    "@cloudant/cloudant": "^2.3.0",
    "assert": "^2.0.0",
    "body-parser": "^1.19.0",
    "body-parser-xml": "^2.0.3",
    "botbuilder-adapter-slack": "^1.0.13",
    "botkit": "^4.10.0",
    "cheerio": "^1.0.0-rc.10",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "errorhandler": "^1.5.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.2",
    "feedparser": "^2.2.10",
    "format": "^0.2.2",
    "helmet": "^4.6.0",
    "ibm-cos-sdk": "^1.10.1",
    "ibm-watson": "^6.1.1",
    "loopback": "^3.22.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^6.4.0",
    "loopback-component-passport": "^3.10.0",
    "loopback-connector-cloudant": "^2.5.0",
    "loopback-context": "^3.4.0",
    "loopback-datasource-juggler": "^4.5.1",
    "loopback-setup-remote-methods-mixin": "^1.0.4",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "mqtt": "^4.2.8",
    "node-cache": "^5.1.2",
    "passport": "^0.2.0",
    "passport-facebook": "^1.0.3",
    "passport-google-oauth": "^0.1.5",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.1.2",
    "passport-twitter": "^1.0.2",
    "path-exists-cli": "^1.0.0",
    "redis": "^3.1.2",
    "replace": "^1.2.1",
    "request": "latest",
    "serve-static": "^1.14.1",
    "socket.io": "^4.1.3",
    "socketio-auth": "^0.1.1",
    "strong-error-handler": "^4.0.0",
    "strong-soap": "^3.2.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "browser-sync": "^2.27.4",
    "concurrently": "^6.2.0",
    "eslint": "^7.31.0",
    "eslint-config-loopback": "^13.1.0",
    "jshint": "^2.13.0",
    "nodemon": "^2.0.12",
    "opener": "^1.5.2",
    "validator": "^13.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/gurvsin3/virtualagent.git"
  },
  "bugs": {
    "url": "https://github.ibm.com/gurvsin3/virtualagent/issues"
  },
