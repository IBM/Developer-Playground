{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models",
      "../node_modules/loopback-component-passport/lib/models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins",
      "../node_modules/loopback-setup-remote-methods-mixin"
    ]
  },
  "MyUser": {
    "dataSource": "accounts",
    "public": true
  },
  "CustomAccessToken": {
    "dataSource": "db",
    "public": false,
    "relations": {
      "user": {
        "type": "belongsTo",
        "foreignKey": "userId"
      }
    }
  },
  "ApplicationCredential": {
    "dataSource": "db",
    "public": false
  },
  "UserCredential": {
    "dataSource": "db",
    "public": false
  },
  "MyUserIdentity": {
    "dataSource": "identities",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "login": true
        }
      }
    }
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "rolemappings",
    "public": true
  },
  "Role": {
    "dataSource": "roles",
    "public": true
  },
  "Conversation": {
    "dataSource": "conversations",
    "public": true
  },
  "Mapping": {
    "dataSource": "mappings",
    "public": true
  }
}
